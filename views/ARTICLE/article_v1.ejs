<script>

  function tous(){
            document.getElementById("container").innerHTML = "";
  
  //LE TABLEAU JS EST PASSé DANS UNE FONCTION
  function afficherTous(tableauDeFilms) {
      tableauDeFilms.forEach(function (viewPost) {
  
  
          //CONTAINER VIEW POST
          var container_view_post= document.createElement('div');
          container_view_post.setAttribute('class', 'row');
          container_view_post.setAttribute('id', 'container_view_post');
          container.appendChild(container_view_post);
  
          //CONTAINER IMG_JOURNAL
          var container_img_journal=document.createElement('div');
          container_img_journal.setAttribute('id', 'img_container_journal');
          container_img_journal.setAttribute('class', 'col');
          container_view_post.appendChild(container_img_journal);
  
    
          //JE CREE DES IMG 
          var journalImage = document.createElement('img');
          journalImage.setAttribute('id', 'img_journal');
          journalImage.setAttribute('class', 'img-fluid');
          journalImage.setAttribute('src', viewPost.url_img);
          container_img_journal.appendChild(journalImage);
  
           
  
          //CONTAINER JOURNAL 
          var containerJournal= document.createElement('div');
          containerJournal.setAttribute('id', 'container_journal');
          containerJournal.setAttribute('class', 'col');
          container_view_post.appendChild(containerJournal);
  
  
          
  
          
  
         //CONTAINER + DATE
          var container_name= document.createElement('div');
          container_name.setAttribute('id', 'container_name');
          
          containerJournal.appendChild(container_name);
  
          //CREE UN PARAGRAPHE NAME
          var p_name= document.createElement('p');
          p_name.setAttribute('id', 'p_name');
          container_name.appendChild(p_name);
  
          //JE CREE DES ZONES DE TEXTE NAME
          var text_name = document.createTextNode( viewPost.titre_article);
          p_name.appendChild(text_name);
  
  
  
          //CONTAINER RESUME
          var container_resume_journal=document.createElement('div');
          container_resume_journal.setAttribute('id', 'container_resume_journal');
          containerJournal.appendChild(container_resume_journal);
  
          //JE CREE DES ZONES DE TEXTE 
